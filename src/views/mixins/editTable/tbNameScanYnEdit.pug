mixin tbNameScanYnEdit(element)
	.tableName_wrapper
		.tableName=`${element.table_name} 스캔여부 : ${element.scan_yn}`
			if element.scan_yn=='Y'
				.edit
					a(href=`/edit-table/${element.table_name}`).tableName_mixin 속성 편집하기
				.scan
					a(href=`/domain-scan/${element.table_name}`).tableName_mixin 도메인 스캔 다시하기
			else
				.scan
					a(href=`/domain-scan/${element.table_name}`).tableName_mixin 도메인 스캔
	br

